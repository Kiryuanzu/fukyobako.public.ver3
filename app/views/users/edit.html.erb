<%= form_with model: @user, local: true do |f| %>
  <h2>名前を変更</h2>
    <div class="form-group">
      <%= f.text_field :username, placeholder: "name(12文字以内)" %>
    </div>
  <h2>プロフィール画像を変更</h2>
    <%= f.file_field :avatar %>
    <%= f.submit "送信", class: "btn btn-primary withripple edit-submit-button" %>
  <% end %>
</div>